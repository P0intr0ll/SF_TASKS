package com.pointroll.events{	import flash.events.Event;	public class PRDataScrapeEvents extends Event {		public static const ON_DATA_SCRAPE_LOAD:String="onDataScrapeLoad";		public static const ON_DATA_SCRAPE_FAIL:String="onDataScrapeFail";		public var params:Object;		public function PRDataScrapeEvents(type:String,data:Object,bubbles:Boolean = false,cancelable:Boolean = false){			super(type,bubbles,cancelable);			this.params = data;		}		public override function clone():Event {			return new  PRDataScrapeEvents(type, params,bubbles, cancelable);		}		public override function toString():String {			return formatToString("CustomEvent", "params", "type", "bubbles", "cancelable");		}	}}