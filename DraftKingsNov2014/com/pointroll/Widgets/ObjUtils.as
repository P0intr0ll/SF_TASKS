package com.PointRoll.Widgets{		import flash.utils.getQualifiedClassName;		public class ObjUtils {				public function ObjUtils() {					}		public static function traceObject(obj:Object, indent:String = ""):String {			var retStr:String = "";			for (var s:String in obj) {				var className:String = flash.utils.getQualifiedClassName( obj[s] );				//var objectClass:Class = flash.utils.getDefinitionByName( className ) as Class;				switch (className) {					case "String":					case "XML":					case "Number":					case "int":						retStr = retStr + indent + s + " is a "+className+" with value = "+obj[s]+"\n";												break;					default:						retStr = retStr + indent + s + " is a "+className+"\n";						if (indent.length > 50) {							retStr += "Line length exceeded.\n";						} else {							retStr += traceObject(obj[s],indent + "    ");						}				}			}			return retStr;		}	}}				