package com.pointroll{ 	import flash.display.*;	import flash.events.*;	import flash.system.Capabilities;			import pointroll.info.getFolderPath;	import pointroll.panel.*; 	import pointroll.*;	 	import com.greensock.TweenLite; 	import com.greensock.plugins.*;		public class PRPanelMain extends MovieClip {		//private var _pr:PointRoll; 		public var close_btn:SimpleButton;		public var cta_btn:SimpleButton;		public var cta_btn2:SimpleButton;		 		public function PRPanelMain() {			addEventListener(Event.ADDED_TO_STAGE, init);		}				private function init(e:Event):void {			removeEventListener(Event.ADDED_TO_STAGE, init); 			pointroll.initAd(this);			 			close_btn.addEventListener(MouseEvent.CLICK, onCloseClick);			cta_btn.addEventListener(MouseEvent.CLICK, onClickTagClick);			//cta_btn2.addEventListener(MouseEvent.CLICK, onClickTagClick2);		}		 		 		private function onCloseClick(e:MouseEvent):void {			play();  					}				private function onClickTagClick(e:MouseEvent):void {							pointroll.launchURL(pointroll.getFlashVar('clickTag1'));		}				private function onClickTagClick2(e:MouseEvent):void {							pointroll.launchURL(pointroll.getFlashVar('clickTag2'));		}					}}