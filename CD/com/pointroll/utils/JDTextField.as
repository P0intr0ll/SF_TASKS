package com.pointroll.utils{	import flash.text.TextField;		import flash.events.FocusEvent;		import com.pointroll.interfaces.ITextField;		public class JDTextField implements ITextField{		private var _textField:TextField;		public function JDTextField(t:TextField){			_textField = t;			setTextField();		}		public function setTextField():void{			_textField.maxChars = 5;			_textField.restrict = "0-9";						_textField.addEventListener(FocusEvent.FOCUS_IN,onTextFieldFocusIn);		}				private function onTextFieldFocusIn(e:FocusEvent):void{			trace("onTextFieldFocusIn");			_textField.text = "";		}	}}